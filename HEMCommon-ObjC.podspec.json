{
  "name": "HEMCommon-ObjC",
  "version": "0.0.9",
  "summary": "Common classes for iOS development in HE:mobile",
  "homepage": "https://github.com/HEmobile/HEMCommon-ObjC",
  "license": "MIT",
  "authors": { 
    "Marcilio Junior": "marciliojrs@gmail.com" 
  },
  "source": {
    "git": "https://github.com/HEmobile/HEMCommon-ObjC.git",
    "branch": "develop",
    "submodules": true
  },
  "subspecs": [
    {
      "name": "Category",
      "source_files": "HEMCommon-ObjC/Category/**/*.{h,m}"
    },
    {
      "name": "CustomCell",
      "source_files": "HEMCommon-ObjC/Cell/**/*.{h,m}"
    },
    {
      "name": "Controller",
      "source_files": "HEMCommon-ObjC/Controller/**/*.{h,m}"
    },
    {
      "name": "Helper",
      "source_files": "HEMCommon-ObjC/Helper/**/*.{h,m}"
    },
    {
      "name": "Network",
      "source_files": "HEMCommon-ObjC/Network/**/*.{h,m}",
      "dependencies": { 
        "AFNetworking": []
      }
    },
    {
      "name": "UI",
      "source_files": "HEMCommon-ObjC/UI/**/*.{h,m}",
      "dependencies": { 
        "JVFloatLabeledTextField": [],
        "CRToast": [],
        "MRProgress": [],
        "MMMaterialDesignSpinner": [],
        "GSIndeterminateProgressBar": []
      }
    }
  ],
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true
}

